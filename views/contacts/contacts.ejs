<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Halaman Contact</title>
  </head>
  <body>
    <table border="1px solid black">
      <thead>
        <th>ID</th>
        <th>NAME</th>
        <th>COMPANY</th>
        <th>TELP</th>
        <th>EMAIL</th>
        <th colspan="3">STATUS</th>
      </thead>
      <tbody>
        <% dataContact.forEach(rows => { %>
          <tr>
            <td><%= rows.id %></td>
            <td><%= rows.name %></td>
            <td><%= rows.company %></td>
            <td><%= rows.telp %></td>
            <td><%= rows.email %></td>
            <td><a href="/contacts/add">ADD CONTACT</a></td>
            <td><a href="/contacts/edit/<%= rows.id %>">EDIT</a></td>
            <td><a href="/contacts/delete/<%= rows.id %>">HAPUS</a></td>
          </tr>
        <% }) %>
      </tbody>
    </table>
  </body>
</html>
