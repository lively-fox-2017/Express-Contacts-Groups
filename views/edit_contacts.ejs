<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Pages Edit Contacts</title>
  </head>
  <body>
  <table>
    <%dataContacts.forEach(row=>{ %>
    <form action="/contacts/edit/<%=row.id%>" method="post">
      Name:<br>
      <input type="text" name="name" value="<%=row.name%>">
      <br>
      Company Names:<br>
      <input type="text" name="company" value="<%=row.company%>">
      <br>
      Telp:<br>
      <input type="text" name="telp_number" value="<%=row.telp_number%>">
      <br>
      Email:<br>
      <input type="text" name="email" value="<%=row.email%>">
      <br>
      <input type="submit" value="Submit">
    </form>
    <%})%>
  </table>
  </body>
</html>
