<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Pages Contacts</title>
  </head>
  <body>
    <tr>
      <h1>MENU</h1>
      <td><a href="/contacts">CONTACTS</a></td>
      <td><a href="/groups">GROUPS</a></td>
      <td><a href="/profiles">PROFILES</a></td>
      <td><a href="/addresses">ADDRESSES</a></td>
    </tr>
  <table border='1px solid'>
    <thead>
      <tr>
        <th>ID</th>
        <th>Street</th>
        <th>City</th>
        <th>Zipcode</th>
        <th>ContactsId</th>
        <th colspan="3">Action</th>
      </tr>
    </thead>
    <tbody>
      <%dataAddress.forEach(row =>{ %>
        <tr>
          <td><%= row.id%></td>
          <td><%= row.street%></td>
          <td><%= row.city%></td>
          <td><%= row.zipcode%></td>
          <td><%= row.name%></td>
          <td><a href="/addresses/add">Add</td>
          <td><a href="/addresses/edit/<%= row.id%>">Edit</td>
          <td><a href="/addresses/delete/<%= row.id%>">Delete</td>
        </tr>
      <%})%>
    </tbody>
  </table>
  </body>
</html>
