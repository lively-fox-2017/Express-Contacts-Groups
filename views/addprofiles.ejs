<!DOCTYPE html>
<html>
<head>
  <title>Welcome | PJAP17</title>
  <link href="https://fonts.googleapis.com/css?family=Playfair+Display:900|Raleway:300" rel="stylesheet" type="text/css">
  <!-- <link rel="stylesheet" type="text/css" href="/assets/style.css"> -->
</head>
  <body>
    <h1>Halaman Add Profiles</h1>
    <% if(dataError) { %>
      <p><%= dataError %></p>
    <% } %>
    <form class="" style="margin-top:10px;" action="/profiles/addprofiles/ %>" method="POST">
      <input type="text" name="username" placeholder="Masukan Nama User">
      <input type="password" name="password" placeholder="Masukan Password">
      <select class="" name="id_contacts">
      <%dataContacts.forEach( row => { %>
        <option value="<%= row.id %>"><%= row.name %></option>
      <% }) %>
      </select>
      <input type="submit" value="SUBMIT">
    </form>
  </body>
</html>
