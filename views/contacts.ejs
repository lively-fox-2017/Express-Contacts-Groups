<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Pages Contacts</title>
  </head>
  <body>
    <tr>
      <h1>MENU</h1>
      <td><a href="/contacts">CONTACTS</a></td>
      <td><a href="/groups">GROUPS</a></td>
      <td><a href="/profiles">PROFILES</a></td>
      <td><a href="/addresses">ADDRESSES</a></td>
    </tr>
  <table border='1px solid'>
    <thead>
      <tr>
        <th>ID</th>
        <th>Names</th>
        <th>Company</th>
        <th>Telp</th>
        <th>Email</th>
        <th colspan="3">Action</th>
      </tr>
    </thead>
    <tbody>
      <%dataContacts.forEach(row =>{ %>
        <tr>
          <td><%= row.id%></td>
          <td><%= row.name%></td>
          <td><%= row.company%></td>
          <td><%= row.telp_number%></td>
          <td><%= row.email%></td>
          <td><a href="/contacts/add">Add</td>
          <td><a href="/contacts/edit/<%= row.id%>">Edit</td>
          <td><a href="/contacts/delete/<%= row.id%>">Delete</td>
        </tr>
      <%})%>
    </tbody>
  </table>
  </body>
</html>
