<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <h1>Ini Adalah Halaman Add Profile</h1>
    <% if(dataError) { %>
      <p><%= dataError %></p>
    <% } %>
  </body>
  <form class="" action="/profiles/add" method="post">
    <input type="text" name="username" placeholder="Input USERNAME Disini">
    <input type="password" name="password" placeholder="Input PASSWORD Disini">
    <select class="" name="id_contacts">
    <%dataContact.forEach( row => { %>
      <option value="<%= row.id %>"><%= row.name %></option>
    <% }) %>
    </select>
    <input type="submit" value="SUBMIT">
  </form>
</html>
