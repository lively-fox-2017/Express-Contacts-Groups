<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Halaman Profiles</title>
  </head>
  <body>
    <table border="2px">
      <thead>
        <tr>
          <td>ID</td>
          <td>Username</td>
          <td>Password</td>
          <td>ID_contacts</td>
          <td>Name_Contacts</td>
          <td>Action</td>
        </tr>
      </thead>
    <tbody>
      <%dataProfiles.forEach(row => { %>
        <tr>
          <td><%=row.id%></td>
          <td><%=row.username%></td>
          <td><%=row.password%></td>
          <td><%=row.idContacts%></td>
          <td><%=row.name%></td>
          <td>
            <a href="/profiles/edit/<%=row.id%>">Edit</a>
            <a href="/profiles/delete/<%=row.id%>">Delete</a>
          </td>
        </tr>
    <%})%>
    </tbody>
  </table>
  </body>
</html>

<form action="/profiles" method="POST">
  Username:<br>
  <input type="text" name="username" placeholder="username">
  <br>
  Password:<br>
  <input type="password" name="password" placeholder="password">
  <br>
  <select class="" name="idContacts">
    <% dataContacts.forEach(row => { %>
     <option value="<%=row.id%>"><%=row.name%></option>
    <% })%>
  </select>
  <label><%=errData%></label>
  <br>
  <input type="submit" value="Submit">
</form>
