<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Pages Profiles</title>
  </head>
  <body>
    <tr>
      <h1>MENU</h1>
      <td><a href="/contacts">CONTACTS</a></td>
      <td><a href="/groups">GROUPS</a></td>
      <td><a href="/profiles">PROFILES</a></td>
      <td><a href="/addresses">ADDRESSES</a></td>
    </tr>
    <table border='1px'>
      <thead>
        <tr>
          <th>ID</th>
          <th>Username</th>
          <th>Password</th>
          <th>idContacts</th>
          <th colspan="3">Action</th>
        </tr>
      </thead>
      <tbody>
        <% dataProfiles.forEach(row=>{ %>
        <tr>
          <td><%= row.id%></td>
          <td><%= row.username%></td>
          <td><%= row.password%></td>
          <td><%= row.name%></td>
          <td><a href="/profiles/add">Add</td>
          <td><a href="/profiles/edit/<%=row.id%>">Edit</td>
          <td><a href="/profiles/delete/<%=row.id%>">Delete</td>
        </tr>
      <% }) %>
      </tbody>
    </table>
  </body>
</html>
