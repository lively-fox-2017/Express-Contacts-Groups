<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Halaman Group</title>
  </head>
  <body>
    <table border="1px solid black">
      <thead>
        <th>ID</th>
        <th>NAME GROUP</th>
        <th colspan="3">STATUS</th>
      </thead>
      <tbody>
        <% dataGroup.forEach(rows => { %>
          <tr>
            <td><%= rows.id %></td>
            <td><%= rows.name_of_group %></td>
            <td><a href="/groups/add">ADD GROUP</a></td>
            <td><a href="/groups/edit/<%= rows.id %>">EDIT</a></td>
            <td><a href="/groups/delete/<%= rows.id %>">HAPUS</a></td>
          </tr>
        <% }) %>
      </tbody>
    </table>
  </body>
</html>
