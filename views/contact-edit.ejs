<%- include('templates/header.ejs') %>
  <div class = "form-container">
    <h3>Edit Contact</h3>
    <form action = "/contacts/<%= datum.id %>/edit" method="POST">
  		Name:<br>
  		<input type = "text" name = "name" value= "<%= datum.name %>">
  		<br>
  		Company:<br>
  		<input type = "text" name = "company" value= "<%= datum.company %>">
  		<br>
  		Phone:<br>
  		<input type = "text" name = "telp_number" value= "<%= datum.telp_number %>">
  		<br>
  		Email:<br>
  		<input type = "text" name = "email" value= "<%= datum.email %>">
  		<br>
  		<br>
  		<input class="waves-effect waves-light btn-large teal accent-4" type = "submit" value = "Submit">
    </form>
  </div>
<%- include('templates/footer.ejs') %>